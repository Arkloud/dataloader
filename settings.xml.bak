<settings>


    <profiles>

    <!-- default core nexus profile: do not remove -->
    <profile>
      <id>nexus</id>

      <properties>
        <!--
            the identifier of the core build workspace = groupId with
            '.' replaced by '_'.
            should not be changed.
        -->
        <id>sfdc.core</id>
        <!--
            p4 branch: set to current branch like 'main' or '184'.
            the value 'none' means this property is not initialized.
        -->    
        <branch>none</branch>
        <!--
            p4 changelist: set to current p4 changelist.
            the value '0' means this property is not initialized.
        -->
        <changelist>0</changelist>
        <!--
            the url of the nexus server to deploy to and resolve from
            should not be changed
        -->
        <nexus.url>https://nexus.soma.salesforce.com/nexus</nexus.url>
        <!-- 
            username part of your nexus user token.
            whenever changing value, also change username and password elements
            in nexus and sfdc.core-... server elements.
        -->
        <nexus.username>zli</nexus.username>
        <!-- 
            password part of your nexus user token.
            whenever changing value, also change username and password elements
            in nexus and sfdc.core-... server elements.
        -->
        <nexus.password>cayman4Nanjing</nexus.password>
        <!--
            nexus branch from which to resolve jars.
            whenever changing value, also change id and url elements of repository
            and plugin repository elements, mirrorOf element and sfdc.core-...
            server id.
            the value 'none' means this property is not initialized.
        -->
        <nexus.branch>none</nexus.branch>
        <!--
            nexus changelist from which to resolve jars.
            whenever changing value, also change id and url elements of repository
            and plugin repository elements, mirrorOf element and sfdc.core-...
            server id.
            the value '0' means this property is not initialized.
        -->
        <nexus.changelist>0</nexus.changelist>
        <!--
            whether or not enable nexus for resolution of pre-built jars.
            whenever changing value, also change value in repository and plugin
            repository releases enabled element.
            may only be set to true if both the nexus branch and nexus changelist
            are initialized.
        -->
        <nexus.enabled>false</nexus.enabled>
      </properties>


 <!-- the repository from which to resolve pre-built jars: do not remove -->
      <repositories>
        <repository>
          <!-- change if needed: ${id}-${nexus.branch}-${nexus.changelist} -->
          <id>sfdc.core-none-0</id>
          <!-- change if needed: ${nexus.url}/content/repositories/${id}-${nexus.branch}-${nexus.changelist} -->
          <url>https://nexus.soma.salesforce.com/nexus/content/repositories/workspaces/sfdc.core/none/0</url>
          <releases>
            <!-- change if needed: ${nexus.enabled} -->
            <enabled>false</enabled>
            <!-- disables metadata refresh: do not change -->
            <updatePolicy>never</updatePolicy>
          </releases>
          <!-- disables shnapshot resolution: do not change -->
          <snapshots>
            <enabled>false</enabled>
            <updatePolicy>never</updatePolicy>
          </snapshots>
        </repository>


      </repositories>

      <!-- the repository from which to resolve pre-built plugin jars: do not remove -->
      <pluginRepositories>
        <pluginRepository>
          <!-- change if needed: ${id}-${nexus.branch}-${nexus.changelist} -->
          <id>sfdc.core-none-0</id>
          <!-- change if needed: ${nexus.url}/content/repositories/${id}-${nexus.branch}-${nexus.changelist} -->
          <url>https://nexus.soma.salesforce.com/nexus/content/repositories/workspaces/sfdc.core/none/0</url>
          <releases>
            <!-- change if needed: ${nexus.enabled} -->
            <enabled>false</enabled>
            <!-- disables metadata refresh: do not change -->
            <updatePolicy>never</updatePolicy>
          </releases>
          <!-- disables shnapshot resolution: do not change -->
          <snapshots>
            <enabled>false</enabled>
            <updatePolicy>never</updatePolicy>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>


  <activeProfiles>
    <!-- activates default nexus profile: do not remove -->
    <activeProfile>nexus</activeProfile>
  </activeProfiles>

  <mirrors>
    <!-- redirects all resolutions to sfdc nexus public group, except for jar repo-->
    <mirror>
      <id>nexus</id>
      <!-- change if needed: *,!${id}-${nexus.branch}-${nexus.changelist} -->
      <mirrorOf>*,!sfdc.sharedlibs,!sfdc.core-none-0</mirrorOf>
      <!-- change if needed: ${nexus.url}/content/groups/public -->
      <url>https://nexus.soma.salesforce.com/nexus/content/groups/public</url>
    </mirror>
  </mirrors>

  <!-- pre-configured plugin groups -->
  <pluginGroups>
    <pluginGroup>org.sonatype.plugins</pluginGroup>
    <pluginGroup>sfdc.provisioning</pluginGroup>
    <pluginGroup>sfdc.workspace</pluginGroup>
    <pluginGroup>com.force.maven.plugin</pluginGroup>
    <pluginGroup>com.force</pluginGroup>
  </pluginGroups>

  <servers>
      <server>
          <id>sonatype-nexus-snapshots</id>
          <username>sfdc_javacloud</username>
          <password>javacloud!@#</password>
       </server>
      <server>
          <id>sonatype-nexus-staging</id>
          <username>sfdc_javacloud</username>
          <password>javacloud!@#</password>
      </server>

    <server>
      <id>sfdc.sharedlibs</id>
      <username>zli</username>
      <password>cayman4Nanjing</password>
    </server>
    <!-- credentials for public group jar resolution -->
    <server>
      <id>nexus</id>
      <!-- change if needed: ${nexus.username} -->
      <username>zli</username>
      <!-- change if needed: ${nexus.password} -->
      <password>cayman4Nanjing</password>
    </server>
    <!-- credentials for nexus pre-built jar resolution -->
    <server>
      <!-- change if needed: ${id}-${nexus.branch}-${nexus.changelist} -->
      <id>sfdc.core-none-0</id>
      <!-- change if needed: ${nexus.username} -->
      <username>zli</username>
      <!-- change if needed: ${nexus.password} -->
      <password>cayman4Nanjing</password>
    </server>
  </servers>
</settings>
